(1 row)

SELECT hashset(h) FROM
(
    SELECT hashset(i) AS h FROM generate_series(1,5) AS i
    UNION ALL
    SELECT hashset(j) AS h FROM generate_series(6,10) AS j
) q;
        hashset         
------------------------
 {8,1,10,3,9,4,6,2,5,7}
(1 row)

